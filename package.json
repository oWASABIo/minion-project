{
  "name": "minions-ai-builder-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=@minions/web",
    "dev:api": "npm run dev --workspace=@minions/api",
    "build": "npm run build --workspaces",
    "build:api": "npm run build:cf --workspace=@minions/api",
    "deploy:api": "cd apps/api && npx wrangler deploy .output/server/index.mjs --assets .output/public --env dev",
    "install:all": "npm install",
    "test": "npm run test --workspaces"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "overrides": {
    "nuxt": "3.15.0",
    "@nuxt/kit": "3.15.0",
    "@nuxt/schema": "3.15.0",
    "nitropack": "2.12.9",
    "@nuxtjs/supabase": "1.1.4",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@pinia/nuxt": "0.5.1"
  }
}
